<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>VL.OpenEXR</id>
    <version>1.0.1</version>
    <title>VL.OpenEXR</title>
    <authors>torinos</authors>
    <owners>torinos</owners>
    <projectUrl>https://github.com/torinos-yt/VL.OpenEXR</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>OpenEXR (.exr) and Radiance HDR (.hdr) image loader and .exr file writer for vvvv</description>
    <tags>VL, Stride, hdr</tags>
    <readme>docs\README.md</readme>
  </metadata>
  <files>
    <file src="..\*.vl" target="" exclude="" />
    <file src="..\help\*.vl" target="\help" exclude="" />
    <file src="..\help\export\**\*.*" target="\help\export" exclude="" />
    <file src="..\help\shaders\*.sdsl" target="\help\shaders" exclude="" />
    <file src="..\resources\*.*" target="\resources" />
    <file src="..\runtimes\**\*.*" target="\runtimes" />
    <file src="..\lib\net8.0\*.dll" target="\lib\net8.0" />
    <file src="..\README.md" target="docs\" />
  </files>
</package>
